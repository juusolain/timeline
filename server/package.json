  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "@types/pouchdb": "^6.4.0",
    "@types/ws": "^7.4.0",
    "json-schema-to-typescript": "^10.1.3",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.5"
  }
